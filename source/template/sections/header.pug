header.header 
   .header__top
      .container
         .contacts 
            a.contact(href="tel:+380969999999")
               i.fa.fa-mobile(aria-hidden="true") 
               span.contact__text 6337 3128
            a.contacts(href="mailto:info @dusiness.co")
               i.fa.fa-envelope(aria-hidden="true")
               span.contact__text info @dusiness.co
            a.facebook-icon(href="facebook.com")
               i.fa.fa-facebook(aria-hidden="true")
   .container
      .header__bottom
         .logo: img.logo__picture(src="images/logo.png" alt="logo")   
         nav.nav 
         -var items =['Home', 'About Us', 'Our Approch', 'Our Work']
            each item in items 
               if item=== 'Home'
                  a.nav__link.nav__link--active(href='#')= item
               else 
                  a.nav__link(href='#')= item 
         a.button.header__button.button--red(href='#') Free Consultation